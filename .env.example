# Claude Flow Docker Environment Configuration
# Copy this file to .env and fill in your values

# NO API KEY NEEDED! Claude Flow uses Claude Code CLI authentication
# (You must have Claude Code installed on your host system)

# User Configuration (automatically set by setup.sh)
USER_UID=1001
USER_GID=1001

# Optional: Customize if needed
PROJECT_NAME=myproject
PACKAGE_MANAGER=pnpm
NODE_VERSION=22
CLAUDE_FLOW_VERSION=alpha